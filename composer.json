{
  "name": "mediatis/formrelay",
  "type": "typo3-cms-extension",
  "description": "Mediatis Formrelay",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3",
    "extension",
    "form",
    "formrelay"
  ],
  "homepage": "https://www.mediatis.de",
  "authors": [
    {
      "name": "Michael VÃ¶hringer",
      "role": "Developer"
    },
    {
      "name": "Meelis Karulin",
      "role": "Developer"
    },
    {
      "name": "Damian Ickler",
      "role": "Developer"
    },
    {
      "name": "Stephan Ude",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "typo3/cms-core": "^9.5 || ^10.4",
    "typo3/cms-form": "^9.5 || ^10.4",
    "mediatis/form_fieldnames": "^2.0 || ^3.1",
    "form-relay/core": "dev-feature/LMSCB-16_async_submission"
  },
  "require-dev": {
    "nimut/testing-framework": "^5.0.3"
  },
  "autoload": {
    "psr-4": {
      "Mediatis\\Formrelay\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mediatis\\Formrelay\\Tests\\": "Tests"
    }
  },
  "replace": {
    "typo3-ter/formrelay": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "link-extension": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/formrelay ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/formrelay"
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
